{
  "name": "mesidor-client",
  "version": "0.11.0",
  "description": "react app frontend for connecting to micron servers",
  "author": "micron-team",
  "main": "webpack.config.js",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack serve --config webpack/config.js --env env=dev",
    "build": "webpack --config webpack/config.js --env env=prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "test": "jest --passWithNoTests --coverage && jest-coverage-badges"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/preset-react": "^7.12.7",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.21.1",
    "browser-detect": "^0.2.28",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "immutability-helper": "^3.1.1",
    "jwt-decode": "^3.1.2",
    "notistack": "^1.0.9",
    "react": "^16.14.0",
    "react-device-detect": "^1.14.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "react-dom": "^16.14.0",
    "react-player": "^2.7.0",
    "react-router-dom": "^5.2.0",
    "react-uuid": "^1.0.2",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@olavoparno/jest-badges-readme": "^1.5.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "lorem-ipsum": "^2.0.3",
    "prettier": "^2.2.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.5.0"
  }
}
